<?xml version="1.0" encoding="UTF-8"?>
<manifest package="com.karansapplication.app" xmlns:android="http://schemas.android.com/apk/res/android">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <application android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name" android:largeHeap="true"
        android:name="com.karansapplication.app.appcomponents.di.MyApp"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true" android:theme="@style/Theme.MyApp" android:usesCleartextTraffic="true">
        <meta-data android:name="com.facebook.sdk.ApplicationId" android:value="@string/facebook_app_id"/>
        <meta-data android:name="com.facebook.sdk.ClientToken" android:value="@string/facebook_client_token"/>
        <activity
            android:configChanges="keyboard|keyboardHidden|screenLayout|screenSize|orientation"
            android:label="@string/app_name" android:name="com.facebook.FacebookActivity"/>
        <activity android:exported="true" android:name="com.facebook.CustomTabActivity">
            <intent-filter>
                <action android:name="android.intent.action.VIEW"/>
                <category android:name="android.intent.category.DEFAULT"/>
                <category android:name="android.intent.category.BROWSABLE"/>
                <data android:scheme="@string/fb_login_protocol_scheme"/>
            </intent-filter>
        </activity>
        <activity
            android:name="com.karansapplication.app.modules.swiperight.ui.SwipeRightActivity" android:screenOrientation="portrait"/>
        <activity
            android:name="com.karansapplication.app.modules.notification.ui.NotificationActivity" android:screenOrientation="portrait"/>
        <activity
            android:name="com.karansapplication.app.modules.swipeleft.ui.SwipeLeftActivity" android:screenOrientation="portrait"/>
        <activity
            android:name="com.karansapplication.app.modules.onboardingtwo.ui.OnboardingTwoActivity" android:screenOrientation="portrait"/>
        <activity
            android:name="com.karansapplication.app.modules.photofullsreen.ui.PhotoFullsreenActivity" android:screenOrientation="portrait"/>
        <activity
            android:name="com.karansapplication.app.modules.match.ui.MatchActivity" android:screenOrientation="portrait"/>
        <activity
            android:name="com.karansapplication.app.modules.friends.ui.FriendsActivity" android:screenOrientation="portrait"/>
        <activity
            android:name="com.karansapplication.app.modules.passions.ui.PassionsActivity" android:screenOrientation="portrait"/>
        <activity android:exported="true"
            android:name="com.karansapplication.app.modules.onboardingone.ui.OnboardingOneActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name="com.karansapplication.app.modules.stories.ui.StoriesActivity" android:screenOrientation="portrait"/>
        <activity
            android:name="com.karansapplication.app.modules.iam.ui.IAmActivity" android:screenOrientation="portrait"/>
        <activity
            android:name="com.karansapplication.app.modules.maincontainer.ui.MainContainerActivity" android:screenOrientation="portrait"/>
        <activity
            android:name="com.karansapplication.app.modules.profileone.ui.ProfileOneActivity" android:screenOrientation="portrait"/>
        <activity
            android:name="com.karansapplication.app.modules.filters.ui.FiltersActivity" android:screenOrientation="portrait"/>
        <activity
            android:name="com.karansapplication.app.modules.signup.ui.SignUpActivity" android:screenOrientation="portrait"/>
        <activity
            android:name="com.karansapplication.app.modules.chat.ui.ChatActivity" android:screenOrientation="portrait"/>
        <activity
            android:name="com.karansapplication.app.modules.code.ui.CodeActivity" android:screenOrientation="portrait"/>
        <activity
            android:name="com.karansapplication.app.modules.profile.ui.ProfileActivity" android:screenOrientation="portrait"/>
        <activity
            android:name="com.karansapplication.app.modules.number.ui.NumberActivity" android:screenOrientation="portrait"/>
        <activity
            android:name="com.karansapplication.app.modules.onboardingthree.ui.OnboardingThreeActivity" android:screenOrientation="portrait"/>
        <activity
            android:name="com.karansapplication.app.modules.swipeleftvtwo.ui.SwipeLeftVtwoActivity" android:screenOrientation="portrait"/>
        <activity
            android:name="com.karansapplication.app.modules.calendar.ui.CalendarActivity" android:screenOrientation="portrait"/>
        <activity
            android:name="com.karansapplication.app.modules.profiledetails.ui.ProfileDetailsActivity" android:screenOrientation="portrait"/>
    </application>
</manifest>
