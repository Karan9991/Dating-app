<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="profileDetailsVM" type="com.karansapplication.app.modules.profiledetails.data.viewmodel.ProfileDetailsVM"/>
    </data>
    <LinearLayout android:gravity="end"
        android:id="@+id/linearProfiledetails"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <TextView android:ellipsize="end" android:id="@+id/txtSkip"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_40pxh"
            android:layout_marginStart="@dimen/_40pxh"
            android:layout_marginTop="@dimen/_60pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{profileDetailsVM.profileDetailsModel.txtSkip}"
            style="@style/txtSkModernistskmodernistbold16" tools:text="@string/lbl_skip"/>
        <TextView android:ellipsize="end" android:gravity="start"
            android:id="@+id/txtProfiledetails"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_124pxh"
            android:layout_marginStart="@dimen/_40pxh"
            android:layout_marginTop="@dimen/_54pxv"
            android:layout_width="match_parent"
            android:singleLine="true"
            android:text="@{profileDetailsVM.profileDetailsModel.txtProfiledetails}"
            style="@style/txtSkModernistskmodernistbold34" tools:text="@string/lbl_profile_details"/>
        <FrameLayout android:id="@+id/frameFace"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_106pxv"
            android:layout_marginEnd="@dimen/_40pxh"
            android:layout_marginStart="@dimen/_40pxh"
            android:layout_marginTop="@dimen/_93pxv" android:layout_width="@dimen/_101pxh">
            <ImageView android:id="@+id/imagePhoto"
                android:layout_gravity="top|center"
                android:layout_height="@dimen/_99pxh"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_marginEnd="@dimen/_2pxh"
                android:layout_width="@dimen/_99pxh"
                android:scaleType="fitXY"
                cornerRadius="@{@dimen/_25pxh}"
                imageRes="@{@drawable/img_photo}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_photo"/>
            <ImageButton
                android:background="@drawable/rectangle_bg_red_400_border_white_a700_radius_17"
                android:id="@+id/btnCamera"
                android:layout_gravity="bottom|end"
                android:layout_height="@dimen/_34pxh"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="@dimen/_34pxh"
                android:paddingHorizontal="@dimen/_9pxh"
                android:paddingVertical="@dimen/_9pxv"
                android:scaleType="fitCenter"
                android:src="@drawable/img_camera" tools:ignore="ContentDescription"/>
        </FrameLayout>
        <FrameLayout android:id="@+id/frameStackalex"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_64pxv"
            android:layout_marginEnd="@dimen/_40pxh"
            android:layout_marginStart="@dimen/_40pxh"
            android:layout_marginTop="@dimen/_39pxv" android:layout_width="@dimen/_295pxh">
            <LinearLayout android:id="@+id/linearColumnalex"
                android:layout_gravity="bottom|start"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylewhite_2">
                <TextView android:ellipsize="end"
                    android:gravity="start" android:id="@+id/txtAlex"
                    android:layout_height="match_parent"
                    android:layout_marginHorizontal="@dimen/_20pxh"
                    android:layout_marginVertical="@dimen/_20pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{profileDetailsVM.profileDetailsModel.txtAlex}"
                    style="@style/txtSkModernistskmodernistregular14_1" tools:text="@string/lbl_david"/>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtTexthere"
                android:layout_gravity="top|start"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_marginEnd="@dimen/_28pxh"
                android:layout_marginStart="@dimen/_28pxh"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{profileDetailsVM.profileDetailsModel.txtTexthere}"
                style="@style/txtSkModernistskmodernistregular12_1" tools:text="@string/lbl_first_name"/>
        </FrameLayout>
        <FrameLayout android:id="@+id/frameStackalexOne"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_64pxv"
            android:layout_marginEnd="@dimen/_40pxh"
            android:layout_marginStart="@dimen/_40pxh"
            android:layout_marginTop="@dimen/_7pxv" android:layout_width="@dimen/_295pxh">
            <LinearLayout android:id="@+id/linearColumnalexOne"
                android:layout_gravity="bottom|start"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylewhite_2">
                <TextView android:ellipsize="end"
                    android:gravity="start" android:id="@+id/txtAlexOne"
                    android:layout_height="match_parent"
                    android:layout_marginHorizontal="@dimen/_20pxh"
                    android:layout_marginVertical="@dimen/_20pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{profileDetailsVM.profileDetailsModel.txtAlexOne}"
                    style="@style/txtSkModernistskmodernistregular14_1" tools:text="@string/lbl_peterson"/>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtTexthereOne"
                android:layout_gravity="top|start"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_marginEnd="@dimen/_28pxh"
                android:layout_marginStart="@dimen/_28pxh"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{profileDetailsVM.profileDetailsModel.txtTexthereOne}"
                style="@style/txtSkModernistskmodernistregular12_1" tools:text="@string/lbl_last_name"/>
        </FrameLayout>
        <LinearLayout android:gravity="center_vertical"
            android:id="@+id/linearInputCalendar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_40pxh"
            android:layout_marginStart="@dimen/_40pxh"
            android:layout_marginTop="@dimen/_10pxv"
            android:layout_width="match_parent"
            android:orientation="horizontal" style="@style/groupStylered_400_63cornerRadius">
            <ImageView android:id="@+id/imageCalendar"
                android:layout_height="@dimen/_20pxh"
                android:layout_marginBottom="@dimen/_19pxv"
                android:layout_marginStart="@dimen/_20pxh"
                android:layout_marginTop="@dimen/_19pxv"
                android:layout_width="@dimen/_18pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_calendar"
                tools:ignore="ContentDescription" tools:src="@drawable/img_calendar"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txt22091997"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_19pxv"
                android:layout_marginEnd="@dimen/_97pxh"
                android:layout_marginStart="@dimen/_19pxh"
                android:layout_marginTop="@dimen/_21pxv"
                android:layout_weight="0.89" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{profileDetailsVM.profileDetailsModel.txt22091997}"
                style="@style/txtSkModernistskmodernistbold14_1" tools:text="@string/msg_choose_birthday"/>
        </LinearLayout>
        <androidx.appcompat.widget.AppCompatButton
            android:ellipsize="end" android:gravity="center"
            android:id="@+id/btnConfirm"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_40pxh"
            android:layout_marginStart="@dimen/_40pxh"
            android:layout_marginTop="@dimen/_88pxv"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_18pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_30pxh"
            android:paddingTop="@dimen/_18pxv" android:singleLine="true"
            android:text="@string/lbl_confirm"
            android:textAllCaps="false" style="@style/btnSolidRounded" tools:text="@string/lbl_confirm"/>
    </LinearLayout>
</layout>
