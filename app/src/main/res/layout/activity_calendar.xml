<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="calendarVM" type="com.karansapplication.app.modules.calendar.data.viewmodel.CalendarVM"/>
    </data>
    <FrameLayout android:id="@+id/frameCalendar"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <ImageView android:id="@+id/imageProfiledetails"
            android:layout_gravity="start"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:scaleType="fitXY"
            imageRes="@{@drawable/img_profiledetails}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_profiledetails"/>
        <FrameLayout android:id="@+id/frameStacklightbulb"
            android:layout_gravity="start"
            android:layout_height="match_parent"
            android:layout_width="match_parent" style="@style/groupStyleblack_900_87">
            <FrameLayout android:id="@+id/frameStackcontainer"
                android:layout_gravity="bottom|start"
                android:layout_height="@dimen/_514pxv"
                android:layout_marginTop="@dimen/_10pxv" android:layout_width="match_parent">
                <ImageView android:id="@+id/imageContainer"
                    android:layout_gravity="start"
                    android:layout_height="match_parent"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_container"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_container"/>
                <LinearLayout android:id="@+id/linearColumnarrowleft"
                    android:layout_gravity="center"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="@dimen/_40pxh"
                    android:layout_marginVertical="@dimen/_40pxv"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <LinearLayout android:id="@+id/linearHeader"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageArrowleft"
                            android:layout_height="@dimen/_8pxh"
                            android:layout_marginBottom="@dimen/_20pxv"
                            android:layout_marginTop="@dimen/_50pxv"
                            android:layout_width="@dimen/_4pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_arrowleft_black_900"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft_black_900"/>
                        <LinearLayout
                            android:gravity="center_horizontal"
                            android:id="@+id/linearColumnbirthday"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_91pxh"
                            android:layout_weight="0.91"
                            android:layout_width="0dp" android:orientation="vertical">
                            <FrameLayout
                                android:id="@+id/frameStackbirthday"
                                android:layout_gravity="start"
                                android:layout_height="@dimen/_57pxv" android:layout_width="@dimen/_85pxh">
                                <TextView android:ellipsize="end"
                                    android:gravity="top|start"
                                    android:id="@+id/txtBirthday"
                                    android:layout_gravity="top|center"
                                    android:layout_height="wrap_content"
                                    android:layout_marginBottom="@dimen/_10pxv"
                                    android:layout_marginEnd="@dimen/_15pxh"
                                    android:layout_marginStart="@dimen/_15pxh"
                                    android:layout_width="match_parent"
                                    android:singleLine="true"
                                    android:text="@{calendarVM.calendarModel.txtBirthday}"
                                    style="@style/txtSkModernistskmodernistregular14_1" tools:text="@string/lbl_birthday"/>
                                <TextView android:ellipsize="end"
                                    android:gravity="top|center"
                                    android:id="@+id/txtZipcode"
                                    android:layout_gravity="bottom|start"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/_10pxv"
                                    android:layout_width="match_parent"
                                    android:singleLine="true"
                                    android:text="@{calendarVM.calendarModel.txtZipcode}"
                                    style="@style/txt_1" tools:text="@string/lbl_1995"/>
                            </FrameLayout>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtJuly"
                                android:layout_gravity="center_horizontal"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_29pxh"
                                android:layout_marginStart="@dimen/_29pxh"
                                android:layout_marginTop="@dimen/_4pxv"
                                android:layout_width="wrap_content"
                                android:singleLine="true"
                                android:text="@{calendarVM.calendarModel.txtJuly}"
                                style="@style/txtSkModernistskmodernistregular14_3" tools:text="@string/lbl_july"/>
                        </LinearLayout>
                        <ImageView android:id="@+id/imageArrowright"
                            android:layout_height="@dimen/_8pxh"
                            android:layout_marginBottom="@dimen/_20pxv"
                            android:layout_marginStart="@dimen/_91pxh"
                            android:layout_marginTop="@dimen/_50pxv"
                            android:layout_width="@dimen/_4pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_arrowright"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright"/>
                    </LinearLayout>
                    <FrameLayout android:id="@+id/frameCalendar1"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_201pxv"
                        android:layout_marginTop="@dimen/_16pxv" android:layout_width="@dimen/_295pxh">
                        <ImageView android:id="@+id/imageOval1line"
                            android:layout_gravity="top|start"
                            android:layout_height="@dimen/_43pxv"
                            android:layout_marginBottom="@dimen/_10pxv"
                            android:layout_width="match_parent"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_oval1line"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_oval1line"/>
                        <LinearLayout android:id="@+id/linearColumnone"
                            android:layout_gravity="bottom|center"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_12pxh"
                            android:layout_marginStart="@dimen/_12pxh"
                            android:layout_marginTop="@dimen/_10pxv"
                            android:layout_width="match_parent" android:orientation="vertical">
                            <androidx.recyclerview.widget.RecyclerView
                                android:id="@+id/recyclerCalendar"
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent"
                                android:orientation="vertical"
                                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                                itemSpace="@{@dimen/_12pxh}"
                                tools:itemCount="4" tools:listitem="@layout/row_calendar"/>
                            <LinearLayout android:gravity="start"
                                android:id="@+id/linearRowtwentynine"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_10pxh"
                                android:layout_marginTop="@dimen/_26pxv"
                                android:layout_width="wrap_content" android:orientation="horizontal">
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtTwentyNine"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="0.36"
                                    android:layout_width="0dp"
                                    android:singleLine="true"
                                    android:text="@{calendarVM.calendarModel.txtTwentyNine}"
                                    style="@style/txtSkModernistskmodernistregular14_1" tools:text="@string/lbl_29"/>
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtThirty"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="@dimen/_24pxh"
                                    android:layout_weight="0.36"
                                    android:layout_width="0dp"
                                    android:singleLine="true"
                                    android:text="@{calendarVM.calendarModel.txtThirty}"
                                    style="@style/txtSkModernistskmodernistregular14_1" tools:text="@string/lbl_30"/>
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtThirtyOne"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="@dimen/_26pxh"
                                    android:layout_weight="0.28"
                                    android:layout_width="0dp"
                                    android:singleLine="true"
                                    android:text="@{calendarVM.calendarModel.txtThirtyOne}"
                                    style="@style/txtSkModernistskmodernistregular14_1" tools:text="@string/lbl_31"/>
                            </LinearLayout>
                        </LinearLayout>
                    </FrameLayout>
                    <androidx.appcompat.widget.AppCompatButton
                        android:ellipsize="end" android:gravity="center"
                        android:id="@+id/btnSave"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_53pxv"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_18pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingStart="@dimen/_30pxh"
                        android:paddingTop="@dimen/_18pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_save"
                        android:textAllCaps="false"
                        style="@style/btnSolidRounded" tools:text="@string/lbl_save"/>
                </LinearLayout>
            </FrameLayout>
            <ImageView android:id="@+id/imageLightbulb"
                android:layout_gravity="top|center"
                android:layout_height="@dimen/_12pxh"
                android:layout_marginBottom="@dimen/_295pxv"
                android:layout_marginEnd="@dimen/_174pxh"
                android:layout_marginStart="@dimen/_174pxh"
                android:layout_marginTop="@dimen/_295pxv"
                android:layout_width="@dimen/_27pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_lightbulb"
                tools:ignore="ContentDescription" tools:src="@drawable/img_lightbulb"/>
        </FrameLayout>
    </FrameLayout>
</layout>
