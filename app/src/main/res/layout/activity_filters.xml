<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="filtersVM" type="com.karansapplication.app.modules.filters.data.viewmodel.FiltersVM"/>
    </data>
    <FrameLayout android:id="@+id/frameFilters"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <ImageView android:id="@+id/imageMain"
            android:layout_gravity="start"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:scaleType="fitXY" imageRes="@{@drawable/img_main}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_main"/>
        <FrameLayout android:id="@+id/frameFilterpopup"
            android:layout_gravity="start"
            android:layout_height="match_parent"
            android:layout_width="match_parent" style="@style/groupStyleblack_900_87">
            <FrameLayout android:id="@+id/frameStackcontainer"
                android:layout_gravity="bottom|start"
                android:layout_height="@dimen/_646pxv"
                android:layout_marginTop="@dimen/_10pxv" android:layout_width="match_parent">
                <ImageView android:id="@+id/imageContainer"
                    android:layout_gravity="start"
                    android:layout_height="match_parent"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_container"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_container"/>
                <LinearLayout android:id="@+id/linearColumnfilters"
                    android:layout_gravity="center"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_34pxv"
                    android:layout_marginEnd="@dimen/_40pxh"
                    android:layout_marginStart="@dimen/_40pxh"
                    android:layout_marginTop="@dimen/_28pxv"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <LinearLayout android:gravity="end"
                        android:id="@+id/linearHeader"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtFilters"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.62"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{filtersVM.filtersModel.txtFilters}"
                            style="@style/txtSkModernistskmodernistbold24" tools:text="@string/lbl_filters"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtSkip"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginStart="@dimen/_73pxh"
                            android:layout_marginTop="@dimen/_7pxv"
                            android:layout_weight="0.38"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{filtersVM.filtersModel.txtSkip}"
                            style="@style/txtSkModernistskmodernistbold16" tools:text="@string/lbl_clear"/>
                    </LinearLayout>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtInterestedin"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_202pxh"
                        android:layout_marginTop="@dimen/_35pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{filtersVM.filtersModel.txtInterestedin}"
                        style="@style/txtSkModernistskmodernistbold16_2" tools:text="@string/lbl_interested_in"/>
                    <LinearLayout android:id="@+id/linearRowgirls"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_20pxv"
                        android:layout_width="match_parent"
                        android:orientation="horizontal" style="@style/groupStylewhite_2">
                        <androidx.appcompat.widget.AppCompatButton
                            android:ellipsize="end"
                            android:gravity="center"
                            android:id="@+id/btnGirls"
                            android:layout_height="@dimen/_58pxv"
                            android:layout_width="@dimen/_98pxh"
                            android:paddingBottom="@dimen/_20pxv"
                            android:paddingEnd="@dimen/_30pxh"
                            android:paddingStart="@dimen/_30pxh"
                            android:paddingTop="@dimen/_20pxv"
                            android:singleLine="true"
                            android:text="@string/lbl_girls"
                            android:textAllCaps="false"
                            style="@style/btnSolidRoundedLeftCorner" tools:text="@string/lbl_girls"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtBoys"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_19pxv"
                            android:layout_marginStart="@dimen/_34pxh"
                            android:layout_marginTop="@dimen/_22pxv"
                            android:layout_weight="0.19"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{filtersVM.filtersModel.txtBoys}"
                            style="@style/txtSkModernistskmodernistregular14_1" tools:text="@string/lbl_boys"/>
                        <View android:background="@color/gray_200"
                            android:id="@+id/lineDividerTwo"
                            android:layout_height="@dimen/_22pxv"
                            android:layout_marginBottom="@dimen/_18pxv"
                            android:layout_marginStart="@dimen/_34pxh"
                            android:layout_marginTop="@dimen/_18pxv"
                            android:layout_weight="0.01" android:layout_width="0dp"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtBoth"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_20pxv"
                            android:layout_marginEnd="@dimen/_34pxh"
                            android:layout_marginStart="@dimen/_33pxh"
                            android:layout_marginTop="@dimen/_20pxv"
                            android:layout_weight="0.19"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{filtersVM.filtersModel.txtBoth}"
                            style="@style/txtSkModernistskmodernistregular14_1" tools:text="@string/lbl_both"/>
                    </LinearLayout>
                    <FrameLayout android:id="@+id/frameStackalex"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_64pxv"
                        android:layout_marginTop="@dimen/_32pxv" android:layout_width="@dimen/_295pxh">
                        <LinearLayout android:id="@+id/linearRowalex"
                            android:layout_gravity="bottom|start"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_10pxv"
                            android:layout_width="match_parent"
                            android:orientation="horizontal" style="@style/groupStylewhite_2">
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtAlex"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_19pxv"
                                android:layout_marginStart="@dimen/_20pxh"
                                android:layout_marginTop="@dimen/_22pxv"
                                android:layout_weight="0.96"
                                android:layout_width="0dp"
                                android:singleLine="true"
                                android:text="@{filtersVM.filtersModel.txtAlex}"
                                style="@style/txtSkModernistskmodernistregular14_1" tools:text="@string/lbl_chicago_usa"/>
                            <ImageView android:id="@+id/imageArrowright"
                                android:layout_height="@dimen/_8pxh"
                                android:layout_marginBottom="@dimen/_25pxv"
                                android:layout_marginEnd="@dimen/_30pxh"
                                android:layout_marginStart="@dimen/_153pxh"
                                android:layout_marginTop="@dimen/_25pxv"
                                android:layout_width="@dimen/_4pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_arrowright_red_400"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright_red_400"/>
                        </LinearLayout>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtTexthere"
                            android:layout_gravity="top|start"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_10pxv"
                            android:layout_marginEnd="@dimen/_28pxh"
                            android:layout_marginStart="@dimen/_28pxh"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{filtersVM.filtersModel.txtTexthere}"
                            style="@style/txtSkModernistskmodernistregular12_1" tools:text="@string/lbl_location"/>
                    </FrameLayout>
                    <LinearLayout android:id="@+id/linearRowdistance"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_33pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtDistance"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.65"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{filtersVM.filtersModel.txtDistance}"
                            style="@style/txtSkModernistskmodernistbold16_2" tools:text="@string/lbl_distance"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtDistanceOne"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginStart="@dimen/_192pxh"
                            android:layout_marginTop="@dimen/_2pxv"
                            android:layout_weight="0.35"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{filtersVM.filtersModel.txtDistanceOne}"
                            style="@style/txtSkModernistskmodernistregular14_4" tools:text="@string/lbl_40km"/>
                    </LinearLayout>
                    <com.google.android.material.slider.Slider
                        android:gravity="start"
                        android:id="@+id/seekBarSlider"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_20pxv"
                        android:layout_width="match_parent"
                        android:value="49" android:valueFrom="0"
                        android:valueTo="100" app:labelBehavior="gone"
                        app:thumbColor="@color/red_400"
                        app:thumbRadius="@dimen/_20pxh"
                        app:trackColorActive="@color/red_400" app:trackColorInactive="@color/gray_200"/>
                    <LinearLayout android:id="@+id/linearRowage"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_35pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtAge"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.43"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{filtersVM.filtersModel.txtAge}"
                            style="@style/txtSkModernistskmodernistbold16_2" tools:text="@string/lbl_age"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txt2028"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_2pxv"
                            android:layout_marginStart="@dimen/_223pxh"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_weight="0.57"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{filtersVM.filtersModel.txt2028}"
                            style="@style/txtSkModernistskmodernistregular14_4" tools:text="@string/lbl_20_28"/>
                    </LinearLayout>
                    <com.google.android.material.slider.RangeSlider
                        android:gravity="start"
                        android:id="@+id/rangeBarSliderOne"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_18pxv"
                        android:layout_width="match_parent"
                        android:valueFrom="0" android:valueTo="100"
                        app:labelBehavior="gone"
                        app:thumbColor="@color/red_400"
                        app:thumbRadius="@dimen/_20pxh"
                        app:trackColorActive="@color/red_400"
                        app:trackColorInactive="@color/gray_200" app:values="@array/range_slider_values"/>
                    <androidx.appcompat.widget.AppCompatButton
                        android:ellipsize="end" android:gravity="center"
                        android:id="@+id/btnContinue"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_40pxv"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_18pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingStart="@dimen/_30pxh"
                        android:paddingTop="@dimen/_18pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_continue"
                        android:textAllCaps="false"
                        style="@style/btnSolidRounded" tools:text="@string/lbl_continue"/>
                </LinearLayout>
            </FrameLayout>
            <ImageView android:id="@+id/imageLightbulb"
                android:layout_gravity="top|center"
                android:layout_height="@dimen/_12pxh"
                android:layout_marginBottom="@dimen/_163pxv"
                android:layout_marginEnd="@dimen/_174pxh"
                android:layout_marginStart="@dimen/_174pxh"
                android:layout_marginTop="@dimen/_163pxv"
                android:layout_width="@dimen/_27pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_lightbulb"
                tools:ignore="ContentDescription" tools:src="@drawable/img_lightbulb"/>
        </FrameLayout>
    </FrameLayout>
</layout>
