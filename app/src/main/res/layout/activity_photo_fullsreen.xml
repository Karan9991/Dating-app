<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="photoFullsreenVM" type="com.karansapplication.app.modules.photofullsreen.data.viewmodel.PhotoFullsreenVM"/>
    </data>
    <FrameLayout android:id="@+id/framePhotofullsreen"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <LinearLayout android:id="@+id/linearColumnarrowleft"
            android:layout_gravity="start"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_44pxv"
            android:layout_marginTop="@dimen/_44pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <ImageButton
                android:background="@drawable/rectangle_bg_white_a700_border_gray_200_radius_15"
                android:id="@+id/btnArrowleft"
                android:layout_gravity="start"
                android:layout_height="@dimen/_52pxh"
                android:layout_marginEnd="@dimen/_40pxh"
                android:layout_marginStart="@dimen/_40pxh"
                android:layout_width="@dimen/_52pxh"
                android:paddingHorizontal="@dimen/_19pxh"
                android:paddingVertical="@dimen/_19pxv"
                android:scaleType="fitCenter"
                android:src="@drawable/img_arrowleft" tools:ignore="ContentDescription"/>
            <ImageView android:gravity="start"
                android:id="@+id/imageFour"
                android:layout_height="@dimen/_562pxv"
                android:layout_marginTop="@dimen/_24pxv"
                android:layout_width="match_parent"
                android:scaleType="fitXY" imageRes="@{@drawable/img_4}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_4"/>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearRowphoto"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_40pxh"
                android:layout_marginStart="@dimen/_40pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imagePhoto"
                    android:layout_height="@dimen/_64pxh"
                    android:layout_width="@dimen/_64pxh"
                    android:scaleType="fitXY"
                    cornerRadius="@{@dimen/_10pxh}"
                    imageRes="@{@drawable/img_4}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_4"/>
                <ImageView android:id="@+id/imagePhotoOne"
                    android:layout_height="@dimen/_54pxh"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:layout_marginStart="@dimen/_11pxh"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_width="@dimen/_54pxh"
                    android:scaleType="fitXY"
                    cornerRadius="@{@dimen/_10pxh}"
                    imageRes="@{@drawable/img_photo_54x54}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_photo_54x54"/>
                <ImageView android:id="@+id/imagePhotoTwo"
                    android:layout_height="@dimen/_54pxh"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_width="@dimen/_54pxh"
                    android:scaleType="fitXY"
                    cornerRadius="@{@dimen/_10pxh}"
                    imageRes="@{@drawable/img_photo_31}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_photo_31"/>
                <ImageView android:id="@+id/imagePhotoThree"
                    android:layout_height="@dimen/_54pxh"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_width="@dimen/_54pxh"
                    android:scaleType="fitXY"
                    cornerRadius="@{@dimen/_10pxh}"
                    imageRes="@{@drawable/img_photo_32}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_photo_32"/>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
</layout>
