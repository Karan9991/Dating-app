                                tools:ignore="ContentDescription" tools:src="@drawable/img_photo_26"/>
                            <ImageView android:id="@+id/imagePhotoTwo"
                                android:layout_height="@dimen/_190pxh"
                                android:layout_marginStart="@dimen/_10pxh"
                                android:layout_width="@dimen/_143pxh"
                                android:scaleType="fitXY"
                                cornerRadius="@{@dimen/_5pxh}"
