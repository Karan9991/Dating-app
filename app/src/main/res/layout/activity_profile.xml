                                android:layout_height="@dimen/_122pxh"
                                android:layout_width="@dimen/_92pxh"
                                android:scaleType="fitXY"
                                cornerRadius="@{@dimen/_5pxh}"
                                imageRes="@{@drawable/img_photo_28}"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_photo_28"/>
                            <ImageView android:id="@+id/imagePhotoFour"
