<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="profileVM" type="com.karansapplication.app.modules.profile.data.viewmodel.ProfileVM"/>
    </data>
    <FrameLayout android:id="@+id/frameStackjessicaparker"
