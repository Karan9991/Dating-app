                    <ImageButton
                        android:background="@drawable/rectangle_bg_white_a700_radius_39"
                        android:id="@+id/btnStar"
                        android:layout_height="@dimen/_78pxh"
                        android:layout_marginBottom="@dimen/_11pxv"
                        android:layout_marginStart="@dimen/_20pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="@dimen/_78pxh"
                        android:paddingHorizontal="@dimen/_26pxh"
                        android:paddingVertical="@dimen/_26pxv"
