                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_weight="0.45"
                                android:layout_width="0dp"
                                android:singleLine="true"
                                android:text="@{profileVM.profileModel.txtReadmoreOne}"
                                style="@style/txtSkModernistskmodernistbold14_1" tools:text="@string/lbl_see_all"/>
