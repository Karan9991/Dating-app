                                android:layout_marginStart="@dimen/_10pxh"
                                android:layout_width="@dimen/_92pxh"
                                android:scaleType="fitXY"
                                cornerRadius="@{@dimen/_5pxh}"
                                imageRes="@{@drawable/img_photo_30}"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_photo_30"/>
