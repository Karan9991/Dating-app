<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="codeRowModel" type="com.karansapplication.app.modules.code.data.model.CodeRowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearRowone"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" android:orientation="horizontal">
        <TextView android:ellipsize="end" android:id="@+id/txtOne"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_1pxv"
            android:layout_weight="0.24" android:layout_width="0dp"
            android:singleLine="true"
            android:text="@{codeRowModel.txtOne}"
            style="@style/txtSkModernistskmodernistregular24" tools:text="@string/lbl_1"/>
        <TextView android:ellipsize="end" android:id="@+id/txtTwo"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_1pxv"
            android:layout_marginStart="@dimen/_111pxh"
            android:layout_weight="0.38" android:layout_width="0dp"
            android:singleLine="true"
            android:text="@{codeRowModel.txtTwo}"
            style="@style/txtSkModernistskmodernistregular24" tools:text="@string/lbl_2"/>
        <TextView android:ellipsize="end" android:id="@+id/txtThree"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_108pxh"
            android:layout_marginTop="@dimen/_1pxv"
            android:layout_weight="0.39" android:layout_width="0dp"
            android:singleLine="true"
            android:text="@{codeRowModel.txtThree}"
            style="@style/txtSkModernistskmodernistregular24" tools:text="@string/lbl_3"/>
    </LinearLayout>
</layout>
