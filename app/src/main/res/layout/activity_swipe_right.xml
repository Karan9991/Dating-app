<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="swipeRightVM" type="com.karansapplication.app.modules.swiperight.data.viewmodel.SwipeRightVM"/>
    </data>
    <FrameLayout android:id="@+id/frameStackphoto"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <LinearLayout android:id="@+id/linearColumnphoto"
            android:layout_gravity="bottom|start"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_34pxv"
            android:layout_marginTop="@dimen/_34pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <FrameLayout android:id="@+id/frameContent"
                android:layout_height="@dimen/_511pxv"
                android:layout_marginStart="@dimen/_10pxh" android:layout_width="@dimen/_293pxh">
                <ImageView android:id="@+id/imagePhoto"
                    android:layout_gravity="start"
                    android:layout_height="match_parent"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    cornerRadius="@{@dimen/_15pxh}"
                    imageRes="@{@drawable/img_photo_511x293}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_photo_511x293"/>
                <LinearLayout android:id="@+id/linearColumnfavorite"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylered_400_75cornerRadius">
                    <ImageButton
                        android:background="@drawable/rectangle_bg_white_a700_radius_48_99"
                        android:id="@+id/btnFavorite"
                        android:layout_gravity="end"
                        android:layout_height="@dimen/_97pxh"
                        android:layout_marginEnd="@dimen/_43pxh"
                        android:layout_marginStart="@dimen/_43pxh"
                        android:layout_marginTop="@dimen/_207pxv"
                        android:layout_width="@dimen/_97pxh"
                        android:paddingHorizontal="@dimen/_23pxh"
                        android:paddingVertical="@dimen/_23pxv"
                        android:scaleType="fitCenter"
                        android:src="@drawable/img_favorite" tools:ignore="ContentDescription"/>
                    <FrameLayout android:id="@+id/frameMaskphoto"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_156pxv"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginTop="@dimen/_49pxv"
                        android:layout_width="@dimen/_293pxh" style="@style/groupStyleblack_900cornerRadius">
                        <ImageView android:id="@+id/imagePhotoOne"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_156pxh"
                            android:layout_width="match_parent"
                            android:scaleType="fitXY"
                            cornerRadius="@{@dimen/_15pxh}"
                            imageRes="@{@drawable/img_photo_156x293}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_photo_156x293"/>
                        <FrameLayout
                            android:id="@+id/frameStackcamilasnowtwentythree"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_156pxv"
                            android:layout_width="@dimen/_293pxh" style="@style/groupStylered_400_1">
                            <TextView android:ellipsize="end"
                                android:gravity="top|start"
                                android:id="@+id/txtCamilaSnowTwentyThree"
                                android:layout_gravity="top|start"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_46pxv"
                                android:layout_marginEnd="@dimen/_26pxh"
                                android:layout_marginStart="@dimen/_26pxh"
                                android:layout_marginTop="@dimen/_46pxv"
                                android:layout_width="match_parent"
                                android:singleLine="true"
                                android:text="@{swipeRightVM.swipeRightModel.txtCamilaSnowTwentyThree}"
                                style="@style/txtSkModernistskmodernistbold24_1" tools:text="@string/lbl_camila_snow_23"/>
                            <TextView android:id="@+id/txtMarketer"
                                android:layout_gravity="top|start"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_47pxv"
                                android:layout_marginEnd="@dimen/_21pxh"
                                android:layout_marginStart="@dimen/_21pxh"
                                android:layout_marginTop="@dimen/_47pxv"
                                android:layout_width="wrap_content"
                                android:text="@{swipeRightVM.swipeRightModel.txtMarketer}"
                                style="@style/txtSkModernistskmodernistregular14" tools:text="@string/lbl_marketer"/>
                        </FrameLayout>
                    </FrameLayout>
                </LinearLayout>
            </FrameLayout>
            <FrameLayout android:gravity="start"
                android:id="@+id/frameBottombar"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" style="@style/groupStylegray_100cornerRadius">
                <FrameLayout android:id="@+id/frameStackmenudivider"
                    android:layout_height="@dimen/_34pxv"
                    android:layout_marginBottom="@dimen/_13pxv" android:layout_width="match_parent">
                    <View android:background="@color/gray_200"
                        android:id="@+id/lineMenudivider"
                        android:layout_height="@dimen/_1pxv"
                        android:layout_marginBottom="@dimen/_10pxv" android:layout_width="@dimen/_375pxh"/>
                    <LinearLayout android:id="@+id/linearRowcomputer"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageComputer"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_19pxh"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_width="@dimen/_22pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_computer"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_computer"/>
                        <ImageView android:id="@+id/imageMenu"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_20pxh"
                            android:layout_marginStart="@dimen/_67pxh"
                            android:layout_width="@dimen/_20pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_menu"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_menu"/>
                        <ImageView android:id="@+id/imageUser"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_18pxh"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginEnd="@dimen/_45pxh"
                            android:layout_marginStart="@dimen/_70pxh"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_width="@dimen/_18pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_user"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_user"/>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linear1Homeactive"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" android:orientation="vertical">
                        <View android:background="@color/red_400"
                            android:id="@+id/lineIndicator"
                            android:layout_height="@dimen/_2pxv" android:layout_width="@dimen/_60pxh"/>
                        <ImageView android:id="@+id/imageFolder"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_21pxh"
                            android:layout_marginTop="@dimen/_11pxv"
                            android:layout_width="@dimen/_22pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_folder"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_folder"/>
                    </LinearLayout>
                </FrameLayout>
            </FrameLayout>
        </LinearLayout>
        <ImageView android:id="@+id/imageMainOne"
            android:layout_gravity="start"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:scaleType="fitXY" imageRes="@{@drawable/img_main1}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_main1"/>
    </FrameLayout>
</layout>
