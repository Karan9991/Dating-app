<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="mainVM" type="com.karansapplication.app.modules.main.data.viewmodel.MainVM"/>
    </data>
    <FrameLayout android:id="@+id/frameStackarrowleft"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/_44pxv" android:layout_width="match_parent">

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:backgroundTint="@color/red_400"
            android:id="@+id/floatingBtnFloatingactionbutton"
            android:layout_gravity="bottom|center"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_138pxh"
            android:layout_marginStart="@dimen/_138pxh"
            android:layout_marginTop="@dimen/_20pxv"
            android:layout_width="wrap_content"
            app:fabCustomSize="@dimen/_99pxh"
            app:srcCompat="@drawable/img_forward" app:tint="@color/white_A700"/>
        <LinearLayout android:id="@+id/linearMain"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_A700">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnarrowleft"
                android:layout_height="0dp"
                android:layout_marginEnd="@dimen/_40pxh"
                android:layout_marginStart="@dimen/_40pxh"
                android:layout_weight="1.0"
                android:layout_width="match_parent" android:orientation="vertical">
                <androidx.appcompat.widget.Toolbar
                    android:id="@+id/toolbarToolbar"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" app:contentInsetStart="0dp">
                    <LinearLayout android:id="@+id/linearRowarrowleft"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <ImageButton
                            android:background="@drawable/rectangle_bg_white_a700_border_gray_200_radius_15"
                            android:id="@+id/btnArrowleft"
                            android:layout_height="@dimen/_52pxh"
                            android:layout_width="@dimen/_52pxh"
                            android:paddingHorizontal="@dimen/_19pxh"
                            android:paddingVertical="@dimen/_19pxv"
                            android:scaleType="fitCenter"
                            android:src="@drawable/img_arrowleft" tools:ignore="ContentDescription"/>
                        <LinearLayout
                            android:gravity="center_horizontal"
                            android:id="@+id/linearColumndiscover"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_2pxv"
                            android:layout_marginStart="@dimen/_46pxh"
                            android:layout_marginTop="@dimen/_5pxv"
                            android:layout_weight="0.49"
                            android:layout_width="0dp" android:orientation="vertical">
                            <TextView android:ellipsize="end"
                                android:gravity="start"
                                android:id="@+id/txtDiscover"
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent"
                                android:singleLine="true"
                                android:text="@{mainVM.mainModel.txtDiscover}"
                                style="@style/txtSkModernistskmodernistbold24" tools:text="@string/lbl_discover"/>
                            <TextView android:ellipsize="end"
                                android:gravity="center_horizontal"
                                android:id="@+id/txtChicagoII"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_20pxh"
                                android:layout_marginStart="@dimen/_20pxh"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_width="match_parent"
                                android:singleLine="true"
                                android:text="@{mainVM.mainModel.txtChicagoII}"
                                style="@style/txtSkModernistskmodernistregular12" tools:text="@string/lbl_chicago_ii"/>
                        </LinearLayout>
                        <ImageButton
                            android:background="@drawable/rectangle_bg_white_a700_border_gray_200_radius_15"
                            android:id="@+id/btnSettings"
                            android:layout_height="@dimen/_52pxh"
                            android:layout_marginStart="@dimen/_45pxh"
                            android:layout_width="@dimen/_52pxh"
                            android:paddingHorizontal="@dimen/_15pxh"
                            android:paddingVertical="@dimen/_15pxv"
                            android:scaleType="fitCenter"
                            android:src="@drawable/img_settings" tools:ignore="ContentDescription"/>
                    </LinearLayout>
                </androidx.appcompat.widget.Toolbar>
                <FrameLayout android:id="@+id/frameContent"
                    android:layout_height="@dimen/_466pxv"
                    android:layout_marginTop="@dimen/_24pxv" android:layout_width="@dimen/_295pxh">
                    <ImageView android:id="@+id/imagePhoto"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_450pxh"
                        android:layout_marginBottom="@dimen/_16pxv"
                        android:layout_marginEnd="@dimen/_32pxh"
                        android:layout_marginStart="@dimen/_32pxh"
                        android:layout_width="@dimen/_231pxh"
                        android:scaleType="fitXY"
                        cornerRadius="@{@dimen/_15pxh}"
                        imageRes="@{@drawable/img_photo_450x231}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_photo_450x231"/>
                    <FrameLayout android:id="@+id/frameStackphotoOne"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_450pxv"
                        android:layout_marginTop="@dimen/_10pxv" android:layout_width="@dimen/_295pxh">
                        <ImageView android:id="@+id/imagePhotoOne"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_450pxh"
                            android:layout_width="match_parent"
                            android:scaleType="fitXY"
                            cornerRadius="@{@dimen/_15pxh}"
                            imageRes="@{@drawable/img_photo_450x295}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_photo_450x295"/>
                        <LinearLayout
                            android:id="@+id/linearColumnlocation"
                            android:layout_gravity="bottom|start"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_10pxv"
                            android:layout_width="match_parent" android:orientation="vertical">
                            <LinearLayout android:gravity="start"
                                android:id="@+id/linearLocation"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_16pxh"
                                android:layout_marginStart="@dimen/_16pxh"
                                android:layout_width="wrap_content"
                                android:orientation="horizontal" style="@style/groupStylewhite_A700_67cornerRadius">
                                <ImageView
                                    android:id="@+id/imageLocation"
                                    android:layout_height="@dimen/_12pxh"
                                    android:layout_marginBottom="@dimen/_10pxv"
                                    android:layout_marginStart="@dimen/_12pxh"
                                    android:layout_marginTop="@dimen/_10pxv"
                                    android:layout_width="@dimen/_9pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_location"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_location"/>
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtDistance"
                                    android:layout_height="wrap_content"
                                    android:layout_marginBottom="@dimen/_8pxv"
                                    android:layout_marginEnd="@dimen/_10pxh"
                                    android:layout_marginStart="@dimen/_4pxh"
                                    android:layout_marginTop="@dimen/_10pxv"
                                    android:layout_weight="0.72"
                                    android:layout_width="0dp"
                                    android:singleLine="true"
                                    android:text="@{mainVM.mainModel.txtDistance}"
                                    style="@style/txtSkModernistskmodernistbold12" tools:text="@string/lbl_1_km"/>
                            </LinearLayout>
                            <ImageView
                                android:id="@+id/imageOverflowmenu"
                                android:layout_gravity="end"
                                android:layout_height="@dimen/_76pxh"
                                android:layout_marginStart="@dimen/_10pxh"
                                android:layout_marginTop="@dimen/_92pxv"
                                android:layout_width="@dimen/_20pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_overflowmenu"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_overflowmenu"/>
                            <FrameLayout
                                android:id="@+id/frameStackphotoTwo"
                                android:layout_gravity="start"
                                android:layout_height="@dimen/_83pxv"
                                android:layout_marginTop="@dimen/_145pxv"
                                android:layout_width="@dimen/_295pxh" style="@style/groupStyleblack_900cornerRadius">
                                <ImageView
                                    android:id="@+id/imagePhotoTwo"
                                    android:layout_gravity="start"
                                    android:layout_height="@dimen/_83pxh"
                                    android:layout_width="match_parent"
                                    android:scaleType="fitXY"
                                    cornerRadius="@{@dimen/_15pxh}"
                                    imageRes="@{@drawable/img_photo_83x295}"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_photo_83x295"/>
                                <LinearLayout
                                    android:id="@+id/linearColumnjessicaparker"
                                    android:layout_gravity="top|start"
                                    android:layout_height="wrap_content"
                                    android:layout_marginBottom="@dimen/_12pxv"
                                    android:layout_marginEnd="@dimen/_16pxh"
                                    android:layout_marginStart="@dimen/_16pxh"
                                    android:layout_marginTop="@dimen/_12pxv"
                                    android:layout_width="wrap_content" android:orientation="vertical">
                                    <TextView android:ellipsize="end"
                                    android:gravity="center_horizontal"
                                    android:id="@+id/txtJessicaParker"
                                    android:layout_height="wrap_content"
                                    android:layout_width="match_parent"
                                    android:singleLine="true"
                                    android:text="@{mainVM.mainModel.txtJessicaParker}"
                                    style="@style/txtSkModernistskmodernistbold24_1" tools:text="@string/msg_jessica_parker"/>
                                    <TextView android:ellipsize="end"
                                    android:id="@+id/txtProfessionalmo"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="@dimen/_10pxh"
                                    android:layout_marginTop="@dimen/_2pxv"
                                    android:layout_width="match_parent"
                                    android:singleLine="true"
                                    android:text="@{mainVM.mainModel.txtProfessionalmo}"
                                    style="@style/txtSkModernistskmodernistregular14" tools:text="@string/msg_professional_mo"/>
                                </LinearLayout>
                            </FrameLayout>
                        </LinearLayout>
                    </FrameLayout>
                </FrameLayout>
                <LinearLayout android:id="@+id/linearButtons"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_21pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageButton
                        android:background="@drawable/rectangle_bg_white_a700_radius_39"
                        android:id="@+id/btnClose"
                        android:layout_height="@dimen/_78pxh"
                        android:layout_marginBottom="@dimen/_11pxv"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="@dimen/_78pxh"
                        android:paddingHorizontal="@dimen/_31pxh"
                        android:paddingVertical="@dimen/_31pxv"
                        android:scaleType="fitCenter"
                        android:src="@drawable/img_close_yellow_900" tools:ignore="ContentDescription"/>

                    <com.google.android.material.floatingactionbutton.FloatingActionButton
                        android:backgroundTint="@color/white_A700"
                        android:id="@+id/floatingBtnFloatingactionbuttonOne"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_11pxv"
                        android:layout_marginStart="@dimen/_139pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_weight="0.5"
                        android:layout_width="0dp"
                        app:srcCompat="@drawable/img_star" app:tint="@color/purple_700"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
</layout>
